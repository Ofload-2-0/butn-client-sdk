{
    "name": "ofload/butn-client",
    "description": "Create a sdk for butn",
    "type": "library",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.0",
        "nesbot/carbon": "^2.68",
        "netresearch/jsonmapper": "^4.2"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "autoload": {
        "psr-4": {
            "Ofload\\Butn\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Basheer Ahmed",
            "email": "basheer.ahmed@ofload.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
